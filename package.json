{
  "name": "heroku-festive",
  "version": "0.1.6",
  "author": "Chad Carbert @chadian",
  "bugs": "https://github.com/chadian/heroku-festive/issues",
  "dependencies": {
    "@oclif/command": "^1.8.7",
    "@oclif/config": "^1.18.2",
    "ink": "^3.2.0",
    "react": "^17.0.2",
    "terminal-image": "^1.2.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.6",
    "@oclif/plugin-help": "^3.2.10",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "chai": "^4",
    "eslint": "^8.4.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^10",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/static",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/chadian/heroku-festive",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "chadian/heroku-festive",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
